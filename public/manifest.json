{
    "manifest_version": 3,
    "version": "1.0",
    "default_locale": "pl",
    "short_name": "Snake",
    "name": "Snake",
    "description": "Strona internetowa na której możesz pobrać grę Snake, co umożliwia rywalizację z innymi użytkownikami, bicie rekordów i wiele więcej...",
    "background": {
        "service_worker": "sw.js"
